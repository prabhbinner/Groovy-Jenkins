pipeline{

    agent any
    
    stages{
    
        stage("Code"){
            steps{
                echo "bhaiya code clone ho gaya"
            }
            
        }
        stage("Build and Test"){
            steps{
                echo "code build ho gaya"
            }
            
        }
        stage("push"){
            steps{
                echo "code push ho gaya"
            }
            
        }
        stage("deploy"){
            steps{
                echo "code deploy ho gaya"
            }
            
        }
        
    }
    
    
}
